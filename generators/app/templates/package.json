{
   "name": "<%_ if (isOpenSource) { _%>@silvermine/<%_ } _%><%= projectName %>",
   "version": "0.1.0",
   "description": "",
   <%_ if (isLibrary) { _%>
   "main": "./dist/commonjs/index",
   "types": "./dist/types/index.d.ts",
   "module": "./dist/esm/index",
   <%_ } _%>
   "scripts": {
      <%_ if (isLibrary || isBrowser) { _%>
      "prepare": "grunt build",
      <%_ } _%>
      "check-node-version": "check-node-version --npm 8.5.5",
      "test": "npm run check-node-version TS_NODE_PROJECT='tests/tsconfig.json' TS_NODE_FILES=true nyc mocha --opts ./.mocha.opts",
      "test:ci": "npm test -- --forbid-only",
      "release:preview": "node ./node_modules/@silvermine/standardization/scripts/release.js preview",
      "release:prep-changelog": "node ./node_modules/@silvermine/standardization/scripts/release.js prep-changelog",
      "release:finalize": "node ./node_modules/@silvermine/standardization/scripts/release.js finalize",
      "commitlint": "commitlint --from $(git rev-list --max-parents=0 HEAD | sed 1q)",
      "markdownlint": "markdownlint -c .markdownlint.json -i CHANGELOG.md '{,!(node_modules)/**/}*.md'",
      "eslint": "eslint '{,!(node_modules|dist)/**/}*.js'",
      "standards": "npm run commitlint && npm run markdownlint && npm run eslint"
   },
   "author": "<%= author %>",
   "license": "<%= isOpenSource ? 'MIT' : 'PROPRIETARY' %>",
   <%_ if (isOpenSource) { _%>
   "repository": {
      "type": "git",
      "url": "git+https://github.com/silvermine/<%= projectName %>.git"
   },
   "bugs": {
      "url": "https://github.com/silvermine/<%= projectName %>/issues"
   },
   "homepage": "https://github.com/silvermine/<%= projectName %>#readme",
   <%_ } _%>
   "devDependencies": {
      "@silvermine/chai-strictly-equal": "1.1.0",
      "@silvermine/eslint-config": "3.0.1",
      "@silvermine/standardization": "2.1.1",
      "@silvermine/typescript-config": "git+https://github.com/silvermine/typescript-config.git#23213e33077089e723629dead5342abe6f3b3c8c",
      "@types/chai": "4.1.7",
      "@types/mocha": "5.2.5",
      "@types/node": "17.0.41",
      "@types/sinon": "5.0.5",
      "coveralls": "3.0.2",
      "chai": "4.2.0",
      "eslint": "6.8.0",
      "grunt": "1.4.1",
      "grunt-cli": "1.3.2",
      <%_ if (isLibrary) { _%>
      "grunt-concurrent": "2.3.1",
      <%_ } _%>
      "grunt-contrib-clean": "2.0.0",
      "grunt-contrib-watch": "1.1.0",
      "grunt-exec": "3.0.0",
      <%_ if (isBrowser) { _%>
      "jsdom": "13.0.0",
      "jsdom-global": "3.0.2",
      <%_ } _%>
      "mocha": "5.2.0",
      "nyc": "13.3.0",
      "sinon": "5.1.1",
      "source-map-support": "0.5.16",
      <%_ if (!isLibrary) { _%>
      "tslib": "1.9.3",
      <%_ } _%>
      "ts-node": "7.0.1",
      <%_ if (isBrowser) { _%>
      "ts-loader": "9.4.2",
      <%_ } _%>
      "typescript": "3.9.5"<% if (isBrowser) { %>,
      "webpack": "5.79.0",
      "webpack-cli": "5.0.1"
      <%_ } _%>
   },
   "dependencies": {
      <%_ if (isLibrary) { _%>
      "tslib": "1.9.3"
      <%_ } _%>
   }
 }
