{
   "name": "<%_ if (isOpenSource) { _%>@silvermine/<%_ } _%><%= projectName %>",
   "version": "0.1.0",
   "description": "",
   <%_ if (isLibrary) { _%>
   "main": "./dist/commonjs/index",
   "types": "./dist/types/index.d.ts",
   "module": "./dist/esm/index",
   <%_ } _%>
   "scripts": {
      <%_ if (isLibrary || isBrowser) { _%>
      "prepare": "grunt build",
      <%_ } _%>
      "test": "check-node-version --npm 6.14.12 && TS_NODE_PROJECT='tests/tsconfig.json' TS_NODE_FILES=true nyc mocha --opts ./.mocha.opts",
      "test:ci": "npm test -- --forbid-only",
      "commitlint": "commitlint --from $(git rev-list --max-parents=0 HEAD | sed 1q)"
   },
   "author": "<%= author %>",
   "license": "<%= isOpenSource ? 'MIT' : 'PROPRIETARY' %>",
   <%_ if (isOpenSource) { _%>
   "repository": {
      "type": "git",
      "url": "git+https://github.com/silvermine/<%= projectName %>.git"
   },
   "bugs": {
      "url": "https://github.com/silvermine/<%= projectName %>/issues"
   },
   "homepage": "https://github.com/silvermine/<%= projectName %>#readme",
   <%_ } _%>
   "devDependencies": {
      "@commitlint/cli": "8.3.5",
      "@commitlint/config-conventional": "8.3.4",
      <%_ if (isOpenSource) { _%>
      "@commitlint/travis-cli": "8.3.5",
      <%_ } _%>
      "@silvermine/chai-strictly-equal": "1.1.0",
      "@silvermine/standardization": "1.2.0",
      "@types/chai": "4.1.7",
      "@types/mocha": "5.2.5",
      "@types/node": "8.10.36",
      "@types/sinon": "5.0.5",
      "coveralls": "3.0.2",
      "chai": "4.2.0",
      "check-node-version": "4.0.2",
      "cz-conventional-changelog": "2.1.0",
      "grunt": "1.4.1",
      "grunt-cli": "1.3.2",
      <%_ if (isLibrary) { _%>
      "grunt-concurrent": "2.3.1",
      <%_ } _%>
      "grunt-contrib-clean": "2.0.0",
      "grunt-contrib-watch": "1.1.0",
      "grunt-eslint": "22.0.0",
      "grunt-exec": "3.0.0",
      <%_ if (isBrowser) { _%>
      "jsdom": "13.0.0",
      "jsdom-global": "3.0.2",
      <%_ } _%>
      "mocha": "5.2.0",
      "nyc": "13.3.0",
      "sinon": "5.1.1",
      "source-map-support": "0.5.16",
      "standard-version": "7.1.0",
      <%_ if (!isLibrary) { _%>
      "tslib": "1.9.3",
      <%_ } _%>
      "ts-node": "7.0.1",
      <%_ if (isBrowser) { _%>
      "ts-loader": "5.3.0",
      <%_ } _%>
      "typescript": "3.2.2"<% if (isBrowser) { %>,
      "webpack": "4.28.3",
      "webpack-cli": "3.2.0"
      <%_ } _%>
   },
   "dependencies": {
      <%_ if (isLibrary) { _%>
      "tslib": "1.9.3"
      <%_ } _%>
   },
   "config": {
      "commitizen": {
         "path": "./node_modules/cz-conventional-changelog"
      }
   }
 }
